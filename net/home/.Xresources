URxvt*termName: rxvt-256color
URxvt.scrollBar: false
URxvt.font: xft:DejaVu Sans Mono:size=10:antialias=true:hinting=true
URxvt*dynamicColors: on
URxvt.cursorBlink: 1
URxvt*geometry: 58x18
URxvt*loginShell: false
URxvt*scrollBar: false
URxvt*saveLines: 65535
URxvt*override-redirect: false
URxvt*internalBorder: 4
URxvt.intensityStyles: false
URxvt.letterSpace: 0
URxvt*shading: 60
URxvt*fading: 0
URxvt*transparent: true

URxvt.perl-ext-common: default,keyboard-select,url-select,clipboard
URxvt.modifier: super
URxvt*perl-lib:         /usr/pkg/lib/urxvt/perl5/
URxvt*perl-ext-common:  default,matcher
URxvt*matcher.button:   1
URxvt*urlLauncher:      /usr/pkg/bin/firefox

URxvt.keysym.M-c:   perl:clipboard:copy
URxvt.keysym.M-v:   perl:clipboard:paste
URxvt.keysym.M-C-v: perl:clipboard:paste_escaped

Xft*dpi:                96
Xft*antialias:          true
Xft*hinting:            none
Xft.autohint:           0
Xft.antialias:          1
Xft.hinting:            true
Xft.hintstyle:          hintfull
Xft.rgba:               rgb
Xft.lcdfilter:          lcddefault

