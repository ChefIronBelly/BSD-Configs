out_to_x no
own_window no
out_to_console yes
background no
max_text_width 0
update_interval 10.0
total_run_times 0
short_units yes
if_up_strictness address
use_spacer left
override_utf8_locale no

TEXT
[
    { "full_text" : "  ${execi 5 ~/.i3/cmus-info} " , "color" : "\#f2f0ec" },
    { "full_text" : "    ${kernel} " , "color" : "\#f2f0ec" },
    { "full_text" : "   ${time %a, %b %d} " , "color" : "\#f2f0ec" },
    { "full_text" : "   ${time %I:%M} " , "color" : "\#f2f0ec" },
    { "full_text" : "" , "color" : "\#f2f0ec" }
],
